{
  "name": "template-react",
  "version": "2.2.0",
  "license": "MIT",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "private": false,
  "repository": "git@github.com:kporten/template-react.git",
  "bugs": "https://github.com/kporten/template-react/issues",
  "description": "Template to develop a single-page application or library with React (TypeScript)",
  "keywords": [
    "template",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.10.0",
    "npm": ">=7.24.0"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview --port 3000",
    "format": "prettier --write --ignore-unknown",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx --max-warnings 0",
    "type-check": "tsc",
    "test": "npm run test:clean && cypress run-ct",
    "test:open": "cypress open-ct",
    "test:coverage": "nyc report --reporter=text --check-coverage --lines 90 --branches 90",
    "test:clean": "rimraf .nyc_output coverage",
    "intl:extract": "formatjs extract 'src/**/*.{ts,tsx}' --format crowdin --throws --out-file .intl_extract/messages.json",
    "intl:upload": "echo 'TODO: Define your translation vendor upload command here (use .intl_extract/messages.json)' && exit 1",
    "intl:download": "echo 'TODO: Define your translation vendor download command here (use .intl_messages)' && exit 1",
    "intl:compile": "formatjs compile-folder .intl_messages src/messages --format crowdin --ast",
    "intl:clean": "rimraf .intl_extract .intl_messages",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.2.21",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "jotai": "^1.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.12",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-angular": "^13.2.0",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/react": "^5.10.0",
    "@cypress/vite-dev-server": "^2.1.0",
    "@formatjs/cli": "^4.3.1",
    "@testing-library/cypress": "^8.0.1",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.6",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-formatjs": "^2.17.8",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "^2.6.2",
    "vite-plugin-istanbul": "^2.2.0"
  }
}
