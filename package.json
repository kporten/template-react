{
  "name": "template-react",
  "description": "Monorepo template for web apps",
  "version": "3.2.3",
  "license": "MIT",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "repository": "https://github.com/kporten/template-react",
  "bugs": "https://github.com/kporten/template-react/issues",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "preview": "turbo run preview",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write --ignore-unknown .",
    "changeset": "changeset add",
    "release": "changeset version && changeset tag",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "2.24.4",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "turbo": "1.4.5"
  }
}
