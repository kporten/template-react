{
  "name": "template-react",
  "version": "1.1.1",
  "license": "MIT",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "private": false,
  "repository": "git@github.com:kporten/template-react.git",
  "bugs": "https://github.com/kporten/template-react/issues",
  "description": "Template to develop a single-page application or library with React (TypeScript)",
  "keywords": [
    "template",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "main": "./dist/template-react.cjs.js",
  "module": "./dist/template-react.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --cache --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "version": "yarn changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-angular": "^12.1.1",
    "@cypress/react": "^5.3.4",
    "@cypress/vite-dev-server": "^1.2.3",
    "@testing-library/cypress": "^7.0.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "@vue/compiler-core": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "^7.1.0",
    "esbuild-runner": "^1.3.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "husky": "^6.0.0",
    "postcss": "^8.2.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "vite": "^2.2.2"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
