{
  "name": "template-react",
  "version": "1.6.0",
  "license": "MIT",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "private": false,
  "repository": "git@github.com:kporten/template-react.git",
  "bugs": "https://github.com/kporten/template-react/issues",
  "description": "Template to develop a single-page application or library with React (TypeScript)",
  "keywords": [
    "template",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview --port 3000",
    "format": "prettier --write --ignore-unknown",
    "lint": "eslint --cache --ignore-path .gitignore --ext .ts,.tsx --max-warnings 0",
    "test": "yarn test:clean && cypress run-ct",
    "test:open": "cypress open-ct",
    "test:clean": "rimraf .nyc_output coverage",
    "test:coverage": "nyc report --reporter=text --check-coverage --lines 90 --branches 90",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "version": "yarn changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@cypress/code-coverage": "^3.9.10",
    "@cypress/react": "^5.9.4",
    "@cypress/vite-dev-server": "^2.0.7",
    "@testing-library/cypress": "^8.0.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.1",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5",
    "vite": "^2.5.0",
    "vite-plugin-istanbul": "^2.2.0"
  }
}
