{
  "name": "template-react",
  "version": "1.0.4",
  "license": "MIT",
  "author": "Kevin Porten <hello@kevinporten.dev>",
  "private": false,
  "repository": "git@github.com:kporten/template-react.git",
  "bugs": "https://github.com/kporten/template-react/issues",
  "description": "Template to develop a single-page application or library with React (TypeScript)",
  "keywords": [
    "template",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "main": "./dist/template-react.cjs.js",
  "module": "./dist/template-react.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --cache --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest --watch --notify",
    "test:ci": "jest --ci --runInBand --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "version": "yarn changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-angular": "^12.0.1",
    "@tailwindcss/jit": "^0.1.13",
    "@testing-library/dom": "^7.30.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^13.0.16",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.5",
    "conventional-changelog-cli": "^2.1.1",
    "esbuild-runner": "^1.3.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "eslint-plugin-unicorn": "^29.0.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3",
    "vite": "^2.1.3"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
