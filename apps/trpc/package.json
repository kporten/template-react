{
  "name": "@template-react/trpc",
  "version": "0.0.0",
  "private": true,
  "types": "./dist/server.d.ts",
  "scripts": {
    "dev": "tsup --watch --onSuccess='node ./dist/server.js'",
    "build": "tsup",
    "preview": "node ./dist/server.js",
    "lint": "eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "typecheck": "tsc",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4.12.0",
    "@trpc/server": "10.18.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.15.11",
    "eslint": "8.37.0",
    "prisma": "4.12.0",
    "tsup": "6.7.0",
    "tsx": "3.12.6",
    "typescript": "5.0.2"
  }
}
