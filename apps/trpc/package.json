  },
  "dependencies": {
    "@prisma/client": "4.10.1",
    "@trpc/server": "10.12.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "6.0.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@template-react/eslint-config": "workspace:0.0.0",
    "@template-react/ts-config": "workspace:0.0.0",
    "@total-typescript/ts-reset": "0.3.7",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.14.1",
    "eslint": "8.34.0",
    "prisma": "4.10.1",
    "tsup": "6.6.3",
    "tsx": "3.12.3",
    "typescript": "4.9.5"
  }
