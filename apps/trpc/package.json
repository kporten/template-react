{
  "name": "@template-react/trpc",
  "version": "0.0.0",
  "private": true,
  "types": "./dist/server.d.ts",
  "scripts": {
    "dev": "tsup --watch --onSuccess='node ./dist/server.js'",
    "build": "tsup",
    "preview": "node ./dist/server.js",
