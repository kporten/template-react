{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5174",
    "build": "vite build",
    "preview": "vite preview --port 5174",
    "lint": "eslint --max-warnings 0 .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:types": "vitest typecheck --run",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract \"src/**/*.{tsx,ts}\" --format crowdin --out-file messages.json --throws",
    "intl:compile": "formatjs compile messages.json --format crowdin --out-file src/messages/en.json",
    "intl:clean": "rm messages.json"
  },
  "dependencies": {
    "@clerk/clerk-react": "4.28.1",
    "@clerk/localizations": "1.26.10",
    "@formatjs/intl-localematcher": "0.5.2",
    "@sentry/react": "7.83.0",
    "@tanstack/react-query": "4.36.1",
    "@tanstack/react-query-devtools": "4.36.1",
    "@template-react/ui-core": "workspace:*",
    "@trpc/client": "10.44.1",
    "@trpc/react-query": "10.44.1",
    "@trpc/server": "10.44.1",
    "jotai": "2.6.0",
    "jotai-devtools": "0.7.0",
    "lucide-react": "0.294.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.1",
    "react-intl": "6.5.5",
    "react-router-dom": "6.20.0",
    "web-vitals": "3.5.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.2.4",
    "@swc-jotai/debug-label": "0.1.0",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/express": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "autoprefixer": "10.4.16",
    "happy-dom": "12.10.3",
    "msw": "2.0.9",
    "postcss": "8.4.31",
    "rollup-plugin-visualizer": "5.9.3",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vitest": "0.34.6"
  }
}
