{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3001",
    "build": "vite build",
    "preview": "vite preview --port 3001",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract 'src/**/*.tsx' --format crowdin --throws --out-file messages.json",
    "intl:compile": "formatjs compile messages.json --format crowdin --out-file src/messages/en.json",
    "intl:clean": "rm messages.json",
    "lint": "eslint --max-warnings 0 --ignore-path ../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.2.25",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@sentry/react": "^6.19.7",
    "jotai": "^1.6.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^5.25.1",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.4",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.12.6",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "jsdom": "^19.0.0",
    "msw": "^0.39.2",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-webfont-dl": "^2.0.0",
    "vitest": "^0.12.6"
  }
}
