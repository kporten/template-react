{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5172",
    "build": "vite build",
    "preview": "vite preview --port 5172",
    "lint": "eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract './src/**/*.tsx' --format=crowdin --throws --out-file=messages.json",
    "intl:compile": "formatjs compile messages.json --format=crowdin --out-file=./src/messages/en.json",
    "intl:clean": "rm messages.json"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.2.32",
    "@headlessui/react": "1.7.11",
    "@heroicons/react": "2.0.16",
    "@tanstack/react-query": "4.24.10",
    "@trpc/client": "10.12.0",
    "@trpc/react-query": "10.12.0",
    "@trpc/server": "10.12.0",
    "@sentry/react": "7.38.0",
    "clsx": "1.2.1",
    "jotai": "2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-intl": "6.2.10",
    "react-router-dom": "6.8.1",
    "tailwind-merge": "1.10.0",
    "web-vitals": "3.1.1"
  },
  "devDependencies": {
    "@formatjs/cli": "6.0.3",
    "@headlessui/tailwindcss": "0.1.2",
    "@template-react/eslint-config": "workspace:0.0.0",
    "@template-react/trpc": "workspace:0.0.0",
    "@template-react/ts-config": "workspace:0.0.0",
    "@testing-library/dom": "9.0.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.3.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.2.0",
    "@vitest/coverage-c8": "0.28.5",
    "@vitest/ui": "0.28.5",
    "autoprefixer": "10.4.13",
    "cross-fetch": "3.1.5",
    "eslint": "8.34.0",
    "happy-dom": "8.7.2",
    "msw": "1.0.1",
    "postcss": "8.4.21",
    "rollup-plugin-visualizer": "5.9.0",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.28.5"
  }
}
