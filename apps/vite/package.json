{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5174",
    "build": "vite build",
    "preview": "vite preview --port 5174",
    "lint": "eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract 'src/**/*.{tsx,ts}' --format=crowdin --out-file=messages.json --throws",
    "intl:compile": "formatjs compile messages.json --format=crowdin --out-file=src/messages/en.json",
    "intl:clean": "rm messages.json"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.2.32",
    "@headlessui/react": "1.7.14",
    "@heroicons/react": "2.0.17",
    "@sentry/react": "7.49.0",
    "@tanstack/react-query": "4.29.5",
    "@trpc/client": "10.21.1",
    "@trpc/react-query": "10.21.1",
    "@trpc/server": "10.21.1",
    "clsx": "1.2.1",
    "jotai": "2.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-intl": "6.4.1",
    "react-router-dom": "6.10.0",
    "tailwind-merge": "1.12.0",
    "web-vitals": "3.3.1"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.0",
    "@headlessui/tailwindcss": "0.1.3",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/express": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@vitejs/plugin-react-swc": "3.3.0",
    "@vitest/coverage-c8": "0.30.1",
    "@vitest/ui": "0.30.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "happy-dom": "8.9.0",
    "msw": "1.2.1",
    "postcss": "8.4.23",
    "rollup-plugin-visualizer": "5.9.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.3",
    "vitest": "0.30.1"
  }
}
