{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5174",
    "build": "vite build",
    "preview": "vite preview --port 5174",
    "lint": "eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:types": "vitest typecheck --run",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract 'src/**/*.{tsx,ts}' --format=crowdin --out-file=messages.json --throws",
    "intl:compile": "formatjs compile messages.json --format=crowdin --out-file=src/messages/en.json",
    "intl:clean": "rm messages.json"
  },
  "dependencies": {
    "@clerk/clerk-react": "4.23.1",
    "@clerk/localizations": "1.24.0",
    "@formatjs/intl-localematcher": "0.4.0",
    "@sentry/react": "7.60.1",
    "@tanstack/react-query": "4.32.0",
    "@template-react/ui-core": "workspace:*",
    "@trpc/client": "10.36.0",
    "@trpc/react-query": "10.36.0",
    "@trpc/server": "10.36.0",
    "jotai": "2.2.2",
    "jotai-devtools": "0.6.0",
    "lucide-react": "0.263.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-intl": "6.4.4",
    "react-router-dom": "6.14.2",
    "web-vitals": "3.4.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.3",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/express": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/coverage-v8": "0.33.0",
    "@vitest/ui": "0.33.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "happy-dom": "10.5.2",
    "msw": "1.2.3",
    "postcss": "8.4.27",
    "rollup-plugin-visualizer": "5.9.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  }
}
