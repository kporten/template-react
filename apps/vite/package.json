{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5172",
    "build": "vite build",
    "preview": "vite preview --port 5172",
    "lint": "TIMING=1 eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract './src/**/*.tsx' --format=crowdin --throws --out-file=messages.json",
    "intl:compile": "formatjs compile messages.json --format=crowdin --out-file=./src/messages/en.json",
    "intl:clean": "rm messages.json"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.2.32",
    "@headlessui/react": "1.7.10",
    "@heroicons/react": "2.0.15",
    "@sentry/react": "7.37.1",
    "clsx": "1.2.1",
    "jotai": "2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-intl": "6.2.8",
    "react-router-dom": "6.8.1",
    "tailwind-merge": "1.9.1",
    "web-vitals": "3.1.1"
  },
  "devDependencies": {
    "@formatjs/cli": "6.0.1",
    "@headlessui/tailwindcss": "0.1.2",
    "@template-react/eslint-config": "workspace:0.0.0",
    "@template-react/ts-config": "workspace:0.0.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react-swc": "3.1.0",
    "@vitest/coverage-c8": "0.28.4",
    "@vitest/ui": "0.28.4",
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "happy-dom": "8.2.6",
    "postcss": "8.4.21",
    "rollup-plugin-visualizer": "5.9.0",
    "tailwindcss": "3.2.6",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.4"
  }
}
