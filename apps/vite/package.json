{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5174",
    "build": "vite build",
    "preview": "vite preview --port 5174",
    "lint": "eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:types": "vitest typecheck --run",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract 'src/**/*.{tsx,ts}' --format=crowdin --out-file=messages.json --throws",
    "intl:compile": "formatjs compile messages.json --format=crowdin --out-file=src/messages/en.json",
    "intl:clean": "rm messages.json"
  },
  "dependencies": {
    "@clerk/clerk-react": "4.21.1",
    "@clerk/localizations": "1.22.0",
    "@formatjs/intl-localematcher": "0.4.0",
    "@radix-ui/react-slot": "1.0.2",
    "@sentry/react": "7.57.0",
    "@tanstack/react-query": "4.29.19",
    "@trpc/client": "10.33.0",
    "@trpc/react-query": "10.33.0",
    "@trpc/server": "10.33.0",
    "class-variance-authority": "0.6.1",
    "clsx": "1.2.1",
    "jotai": "2.2.1",
    "jotai-devtools": "0.6.0",
    "lucide-react": "0.257.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-intl": "6.4.4",
    "react-router-dom": "6.14.1",
    "tailwind-merge": "1.13.2",
    "web-vitals": "3.3.2"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.3",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/express": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/coverage-v8": "0.32.2",
    "@vitest/ui": "0.32.2",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "happy-dom": "9.20.3",
    "msw": "1.2.2",
    "postcss": "8.4.24",
    "rollup-plugin-visualizer": "5.9.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  }
}
