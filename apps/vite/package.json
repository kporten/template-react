{
  "name": "@template-react/vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5172",
    "build": "vite build",
    "preview": "vite preview --port 5172",
    "intl": "pnpm run intl:extract && pnpm run intl:compile && pnpm run intl:clean",
    "intl:extract": "formatjs extract 'src/**/*.tsx' --format=crowdin --throws --out-file=messages.json",
    "intl:compile": "formatjs compile messages.json --format=crowdin --out-file=src/messages/en.json",
    "intl:clean": "rimraf messages.json",
    "lint": "TIMING=1 eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.2.31",
    "@headlessui/react": "1.7.0",
    "@heroicons/react": "2.0.10",
    "@sentry/react": "7.12.1",
    "jotai": "1.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-intl": "6.1.1",
    "react-router-dom": "6.3.0",
    "web-vitals": "3.0.1"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.0",
    "@headlessui/tailwindcss": "0.1.1",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@testing-library/dom": "8.17.1",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "@vitest/coverage-c8": "0.23.2",
    "@vitest/ui": "0.23.2",
    "autoprefixer": "10.4.8",
    "eslint": "8.23.0",
    "jsdom": "20.0.0",
    "postcss": "8.4.16",
    "rimraf": "3.0.2",
    "tailwindcss": "3.1.8",
    "typescript": "4.9.5",
    "vite": "3.1.0",
    "vite-plugin-webfont-dl": "3.1.1",
    "vitest": "0.23.2"
  }
}
