# required, used to connect to the database
DATABASE_URL="mysql://root:local@localhost:3306/keystone"
# docker connection string
# DATABASE_URL="mysql://root:local@db:3306/keystone"

# optional, recommended in production, set runtime environment (default is development)
# NODE_ENV=production

# optional, used as server port (default is 5171, see https://keystonejs.com/docs/config/config#server)
# PORT=5171

# optional, recommended in production, used for cookie encryption (default is auto-generated)
# SESSION_SECRET="-- KEYSTONE SESSION SECRET; CHANGE ME --"

# optional, change all required process.env values to be optional
# SKIP_ENV_REQUIRED=1
