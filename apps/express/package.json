{
  "name": "@template-react/express",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch --onSuccess='node ./dist/server.js'",
    "build": "tsup",
    "preview": "node ./dist/server.js",
    "lint": "eslint --max-warnings=0 --ignore-path=../../.gitignore .",
    "test:types": "tsc",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "4.12.2",
    "@prisma/client": "5.1.0",
    "@trpc/server": "10.36.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "pino": "8.14.2",
    "pino-http": "8.3.3",
    "pino-pretty": "10.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.17.1",
    "eslint": "8.46.0",
    "prisma": "5.1.0",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
