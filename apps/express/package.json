    "express": "4.18.2",
    "helmet": "7.0.0",
    "pino": "8.16.1",
    "pino-http": "8.5.1",
    "pino-pretty": "10.2.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@template-react/eslint-config": "workspace:*",
    "@template-react/ts-config": "workspace:*",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/cors": "2.8.15",
    "@types/express": "4.17.20",
    "@types/node": "20.8.9",
    "eslint": "8.52.0",
